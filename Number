import java.util.Scanner;

public class Number {
  
  public static void main(String []args) {
    
    Scanner scanner = new Scanner(System.in);
    int num = scanner.nextInt();
    
    if(num > 0)
      System.out.print("Positive");
      
    else if(num < 0)
      System.out.print("Negative");
      
    else
      System.out.print("Zero");
  }
}
